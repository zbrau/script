-- =======================================================================
-- INTERFAZ CON PESTA√ëAS POR CATEGOR√çAS
-- =======================================================================
local Window = Rayfield:CreateWindow({
    Name = "üö¥ Cambiador de Bicis Pro",
    LoadingTitle = "Cargando 200 bicis...",
})

-- Toggle global
local MainTab = Window:CreateTab("‚öôÔ∏è Control", 4483362458)
MainTab:CreateToggle({
    Name = "üü¢ Activar Auto-Cambio",
    CurrentValue = true,
    Callback = function(value)
        activo = value
        warn(value and "‚úì Sistema ACTIVADO" or "‚ö† Sistema DESACTIVADO")
    end,
})

MainTab:CreateParagraph({
    Title = "üí° Instrucciones",
    Content = "1. Busca tu bici en las pesta√±as\n2. Presiona el bot√≥n de la bici\n3. Resetea (R) para aplicar"
})

-- Funci√≥n para crear bot√≥n de bici
local function crearBotonBici(tab, id, nombre)
    tab:CreateButton({
        Name = string.format("#%d - %s", id, nombre),
        Callback = function()
            ID_A_FORZAR = id
            warn(string.format("‚úì Bici seleccionada: #%d - %s", id, nombre))
            warn("‚ö† Presiona R para resetear y aplicar")
        end,
    })
end

-- B√ÅSICAS (1-20)
local TabBasicas = Window:CreateTab("üü¢ B√°sicas (1-20)", 4483362458)
TabBasicas:CreateSection("Bicis Comunes")
for i = 1, 20 do
    crearBotonBici(TabBasicas, bicis[i][1], bicis[i][2])
end

-- INTERMEDIAS (21-50)
local TabIntermedias = Window:CreateTab("üîµ Intermedias (21-50)", 4483362458)
TabIntermedias:CreateSection("Bicis Poco Comunes")
for i = 21, 50 do
    crearBotonBici(TabIntermedias, bicis[i][1], bicis[i][2])
end

-- RARAS (51-100)
local TabRaras = Window:CreateTab("üü£ Raras (51-100)", 4483362458)
TabRaras:CreateSection("Bicis Raras - Parte 1")
for i = 51, 75 do
    crearBotonBici(TabRaras, bicis[i][1], bicis[i][2])
end
TabRaras:CreateSection("Bicis Raras - Parte 2")
for i = 76, 100 do
    crearBotonBici(TabRaras, bicis[i][1], bicis[i][2])
end

-- √âPICAS (101-150)
local TabEpicas = Window:CreateTab("üü† √âpicas (101-150)", 4483362458)
TabEpicas:CreateSection("Bicis √âpicas - Parte 1")
for i = 101, 125 do
    crearBotonBici(TabEpicas, bicis[i][1], bicis[i][2])
end
TabEpicas:CreateSection("Bicis √âpicas - Parte 2")
for i = 126, 150 do
    crearBotonBici(TabEpicas, bicis[i][1], bicis[i][2])
end

-- LEGENDARIAS (151-200)
local TabLegendarias = Window:CreateTab("üî¥ Legendarias (151-200)", 4483362458)
TabLegendarias:CreateSection("Bicis Legendarias - Parte 1")
