    crearBotonBici(TabLegendarias, bicis[i][1], bicis[i][2])
end
TabLegendarias:CreateSection("Bicis Legendarias - Parte 2")
for i = 176, 200 do
    crearBotonBici(TabLegendarias, bicis[i][1], bicis[i][2])
end

-- =======================================================================
-- SISTEMA DE CAMBIO (IGUAL QUE ANTES)
-- =======================================================================
local function aplicarCambioBici(character)
    if not activo then return end
    
    task.spawn(function()
        local hrp = character:WaitForChild("HumanoidRootPart", 10)
        if not hrp then return end
        
        local bikeType = hrp:WaitForChild("BikeType", 10)
        if not bikeType then return end
        
        bikeType.Value = ID_A_FORZAR
        
        -- Monitor constante
        local conexion
        conexion = bikeType:GetPropertyChangedSignal("Value"):Connect(function()
            if activo and bikeType.Value ~= ID_A_FORZAR then
                bikeType.Value = ID_A_FORZAR
            end
        end)
        
        character.Humanoid.Died:Connect(function()
            conexion:Disconnect()
        end)
    end)
end

-- Hook del m√≥dulo
task.spawn(function()
    local BikeModule = require(ReplicatedStorage.Modules.Bike)
    local originalAttach = BikeModule.Attach
    
    BikeModule.Attach = function(self, char)
        if activo then
            task.spawn(function()
                local bikeType = char:WaitForChild("HumanoidRootPart"):WaitForChild("BikeType")
                bikeType.Value = ID_A_FORZAR
            end)
        end
        return originalAttach(self, char)
    end
end)

player.CharacterAdded:Connect(aplicarCambioBici)
if player.Character then
    aplicarCambioBici(player.Character)
end

-- Loop de verificaci√≥n
task.spawn(function()
    while task.wait(5) do
        if activo then
            local char = player.Character
            if char then
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local bikeType = hrp:FindFirstChild("BikeType")
                    if bikeType and bikeType.Value ~= ID_A_FORZAR then
                        bikeType.Value = ID_A_FORZAR
                    end
                end
            end
        end
    end
end)

print("‚úÖ Sistema cargado con 200 bicis organizadas")
print("üìÇ Usa las pesta√±as para navegar por categor√≠as")
